# Root gitignore for Smart Business AI Assistant

# Node.js
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
package-lock.json.*

# Build outputs
dist/
build/
coverage/
.cache/
*.tsbuildinfo

# Next.js
.next/
out/

# IDE/OS
.DS_Store
Thumbs.db
.idea/
.vscode/

# Env files (keep examples)
.env
.env.*
!.env.example
.vscode/settings.json
backend/.env
backend/.env.*
!backend/.env.example
packages/**/.env
packages/**/.env.*
