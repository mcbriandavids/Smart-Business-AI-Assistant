{
  "name": "smart-business-ai-workspace",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently -n backend,frontend -c \"bgBlue.bold\",\"bgGreen.bold\" \"npm:dev --workspace backend\" \"npm:dev --workspace frontend\"",
    "dev:backend": "npm run dev --workspace backend",
    "dev:frontend": "npm run dev --workspace frontend"
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  }
}
