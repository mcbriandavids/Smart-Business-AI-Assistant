{
  "name": "smart-business-ai-backend",
  "version": "1.0.0",
  "description": "AI-powered small business marketplace backend API",
  "main": "src/server.js",
  "scripts": {
    "start": "node --no-deprecation src/server.js",
    "dev": "cross-env PORT=3000 nodemon --exec \"node --no-deprecation\" src/server.js",
    "test": "mocha --require tests/setup.js --timeout 600000 \"tests/**/*.test.js\"",
    "coverage": "nyc --reporter=text-summary --reporter=lcov --reporter=json-summary npm test",
    "seed": "node src/scripts/seed.js",
    "check:filenames": "node src/scripts/check-filenames.js"
  },
  "keywords": [
    "ai",
    "marketplace",
    "business",
    "express",
    "mongodb"
