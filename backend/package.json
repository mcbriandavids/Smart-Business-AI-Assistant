    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.7.2",
    "multer": "^1.4.5-lts.1",
    "joi": "^17.9.2",
    "nodemailer": "^6.9.4",
    "twilio": "^4.15.0",
    "axios": "^1.5.0",
    "moment": "^2.29.4",
    "uuid": "^9.0.0",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "pino": "^9.3.1",
    "pino-http": "^9.0.0",
